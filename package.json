{
  "name": "vibe",
  "version": "0.1.0",
  "description": "A fun TypeScript library that uses LLM to dynamically generate function implementations",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "test": "bun run src/tests/index.ts",
    "playground": "bun run src/examples/playground.ts",
    "build": "tsc",
    "game": "bun run src/examples/game/game-entry.ts",
    "game-server": "bun run src/examples/game/server.ts",
    "game-demo": "bun run src/examples/game/game-entry.ts"
  },
  "keywords": [
    "llm",
    "ai",
    "dynamic",
    "proxy",
    "zod"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "express": "^5.2.1",
    "openai": "^4.20.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "zod": "^3.22.4"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  }
}
