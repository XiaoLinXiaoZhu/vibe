{
  "name": "vibe",
  "version": "0.1.0",
  "description": "A fun TypeScript library that uses LLM to dynamically generate function implementations",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "example": "bun run src/example.ts",
    "test": "bun run src/test.ts",
    "log-example": "bun run src/log-example.ts",
    "test-llm-log": "bun run src/test-llm-log.ts",
    "build": "tsc"
  },
  "keywords": [
    "llm",
    "ai",
    "dynamic",
    "proxy",
    "zod"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "zod": "^3.22.4",
    "openai": "^4.20.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "zod": "^3.22.4"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  }
}
